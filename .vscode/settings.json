{
    "rust-analyzer.cargo.target": "aarch64-unknown-none",
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.diagnostics.disabled": [
        "macro-error"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/**": true
    }
}